   "source": [
    "import pandas as pd\n",
    "df = pd.read_csv('train.csv')\n",
    "\n",
    "review_dict = dict()\n",
    "cnt = 0\n",
    "for row in df.itertuples():\n",
    "    if row.review in review_dict.keys():\n",
    "        cnt += 1\n",
    "        continue\n",
    "    review_dict[row.review] = row.rating"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "print(cnt)"
   ]
